{
    "title": "Book",
    "description": "开源、共享、积累",
    "author": "Anchep",
    "output.name": "Book",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
        "sidebar": {
            "返回网站": "https://www.280i.com"
        }
    },
    "plugins": [
        "code",
        "-search",
        "search-pro",
        "github",
        "splitter",
        "tbfed-pagefooter",
        "donate",
        "prism",
        "-highlight",
        "expandable-chapters",
        "multipart",
        "ancre-navigation",
        "hide-element"
    ],

    "pluginsConfig": {
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "github": {
            "url": "https://github.com/anchep"
        },
        "code": {
            "copyButtons": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © 280i.com 2020",
            "modify_label": "本书建立时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "donate": {
            "wechat": "http://www.280i.com/book/images/wx.png",
            "alipay": "http://www.280i.com/book/images/alipay.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ],
            "lang": {
                "flow": "typescript"
            }
        }
    }

   
}
 